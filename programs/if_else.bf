// "A\nB\n"[set][a]
+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++>  // A
++++++++++> // \n
++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++> // B
++++++++++> // \n
[-] // set = 0
+ // set = 1
> // mv a
[-] // a = 0
, // input
------------------------------------------------ // ascii -> int
- // a = a - 1
[ // if a != 1
<<<.>.// print "B\n"
>[-] // set = 0
>[-] // a = 0
]
< // mv set
[ // else (set != 0)
<<<<.>.// print "A"
>>>[-] // set = 0
]
